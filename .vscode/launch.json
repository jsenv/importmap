{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "node",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${file}",
      "runtimeVersion": "13.6.0",
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        // force-array-multiline
        "node_modules/@babel/**",
        "node_modules/@jsenv/cancellation/**",
        "node_modules/regenerator-trasform/**",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
