{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "node",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${file}",
      "runtimeArgs": [],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        "node_modules/**",
        "node_modules/@babel/template/**",
        "node_modules/@rollup/**",
        "<node_internals>/**"
      ]
    }
  ]
}
