{
  "name": "@jsenv/importmap",
  "version": "1.0.0",
  "description": "Helpers to implement importmaps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap"
  },
  "engines": {
    "node": ">=12.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/commonjs/jsenv_importmap.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/commonjs/jsenv_importmap.cjs",
  "files": [
    "/dist/",
    "/src/",
    "/index.js"
  ],
  "scripts": {
    "eslint-check": "node ./node_modules/eslint/bin/eslint.js .",
    "generate-importmap": "node ./script/generate-importmap/generate-importmap.js",
    "dist": "npm run build-esmodule && npm run build-commonjs",
    "build-esmodule": "node --experimental-import-meta-resolve ./script/build/build-esmodule.js",
    "build-commonjs": "node --experimental-import-meta-resolve ./script/build/build-commonjs.js",
    "start-exploring": "node ./script/start-exploring/start-exploring.js",
    "test": "node --unhandled-rejections=strict ./script/test/test.js",
    "test-with-coverage": "npm run test -- --coverage",
    "prettier-format": "node ./script/prettier-format/prettier-format.js",
    "prettier-format-stage": "npm run prettier-format -- --staged",
    "prettier-check": "npm run prettier-format -- --dry-run",
    "upload-coverage": "node ./script/upload-coverage/upload-coverage.js",
    "postinstall": "npm run generate-importmap",
    "prepublishOnly": "node ./script/transform-package/remove-postinstall.js && npm run dist",
    "postpublish": "node ./script/transform-package/restore-postinstall.js"
  },
  "dependencies": {
    "@jsenv/logger": "4.0.1"
  },
  "devDependencies": {
    "@jsenv/assert": "2.2.5",
    "@jsenv/codecov-upload": "3.5.0",
    "@jsenv/core": "19.0.2",
    "@jsenv/eslint-config": "15.0.1",
    "@jsenv/github-release-package": "1.2.3",
    "@jsenv/importmap-eslint-resolver": "5.0.0",
    "@jsenv/node-module-import-map": "13.6.1",
    "@jsenv/package-publish": "1.6.2",
    "@jsenv/prettier-check-project": "5.6.1",
    "eslint": "7.31.0",
    "eslint-plugin-import": "2.23.4",
    "playwright-chromium": "1.13.0",
    "playwright-firefox": "1.13.0",
    "playwright-webkit": "1.13.0",
    "prettier": "2.3.2"
  }
}
