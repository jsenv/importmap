{
  "name": "@jsenv/importmap",
  "version": "1.2.0",
  "description": "Helpers to implement importmaps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js",
      "require": "./dist/commonjs/jsenv_importmap.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/commonjs/jsenv_importmap.cjs",
  "files": [
    "/dist/",
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "dev": "node ./script/server/start_dev_server.mjs",
    "eslint-check": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.mjs,.html",
    "importmap-generate": "node ./script/importmap/generate_importmap.mjs",
    "dist": "npm run build-esmodule && npm run build-commonjs",
    "build-esmodule": "node --experimental-import-meta-resolve ./script/build/build_esmodule.mjs",
    "build-commonjs": "node --experimental-import-meta-resolve ./script/build/build_commonjs.mjs",
    "test": "node ./script/test/test.mjs",
		"test-with-coverage": "npm run test -- --coverage",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
		"postinstall": "npm run importmap-generate",
		"prepublishOnly": "node ./script/publish/remove_postinstall.mjs && npm run dist",
		"postpublish": "node ./script/publish/restore_postinstall.mjs"
  },
  "dependencies": {
    "@jsenv/logger": "4.0.1"
  },
  "devDependencies": {
    "@jsenv/assert": "2.3.2",
    "@jsenv/core": "23.8.7",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/github-release-package": "1.2.3",
    "@jsenv/importmap-eslint-resolver": "5.1.2",
    "@jsenv/importmap-node-module": "2.6.0",
    "@jsenv/package-publish": "1.6.2",
    "eslint": "8.2.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.3",
    "playwright": "1.16.3",
    "prettier": "2.4.1"
  }
}
